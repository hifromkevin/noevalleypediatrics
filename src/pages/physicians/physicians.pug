.pages-physicians
  Loader(v-show='loading')
  div(v-if='props')
    //pre {{ props }}
  Hero(:props='props.acf.hero')
  .pages-physicians__hero(:style='{ backgroundImage: "url(" + props.acf.hero + ")" }')
    .col-sm-3.col-sm-offset-8.hero-holder
      h1.hero-title 
        | {{ props.acf.hero_title }}
      h1.hero-secondary
        | {{ props.acf.hero_title_2 }}
  .pages-physicians__container
    .row.bio
      .col-sm-4
        img(:src='props.acf.doctor_photo_1')

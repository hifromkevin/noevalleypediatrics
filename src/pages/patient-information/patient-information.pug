.pages-information
  Loader(v-show='loading')
  div(v-if='props')
    //- pre {{ props }}
  Hero(:props='props.acf.insurance_hero')
  .pages-information__hero(:style='{ backgroundImage: "url(" + props.acf.hero + ")" }')
    .col-sm-3.col-sm-offset-8.hero-holder
      h1.hero-title 
        | {{ props.acf.hero_title }}
      h1.hero-secondary
        | {{ props.acf.hero_title_2 }}
  .pages-information__container
    .row
      .col-sm-3
        h2.title 
          | {{ props.acf.title_1 }}
        h2.secondary-title
          | {{ props.acf.title_1a }}
      .col-sm-9(v-html='props.acf.text_1')
        | {{ props.acf.text_1 }}
